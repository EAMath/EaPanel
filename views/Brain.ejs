              omurilik: "<b>Omurilik</b> <em>Beyin ile kol, gövde arasında irtibatı sağlayan ve beyinden bu bölgelerdeki sinir dokusundan oluşur.</em>",
              hipotalamus: "<b>Hipotalamus</b> <em>Vücut sıcaklığı mekanizmasını ve Susama, acıkma, ve kan basıncını ayarlar.</em>",
              hipofiz: "<b>Hipofiz</b> <em>Ön ve arka bölümünden farklı hormonlar salgılayarak tiroid bezi, böbrek üstü bezi, vb çalışmalarını düzenler.</em>",
              talamus: "<b>Talamus</b> <em>Koku duyusu hariç, tüm sistemlerden gelen sinyaller için bir kapı olarak kabul edilir</em>",
              epitalamus: "<b>Epitalamus</b> <em>Limbik sistemi beynin diğer bölümlerine bağlamaktır.</em>",
              nasırlıcisim: "<b>Nasırlı Cisim</b> <em>Nasırlı Cisim beyin yarıküreleri arası iletişimi sağlar.</em>",
              hayatucgeni: "<b>Hayat Üçgeni</b> <em></em>",
              frontallob: "<b>Frontal Lob</b> <em>Beyninimizin ana kumanda merkezidir. hafızanın yönetimi, karar verme ve itkilerin kontrolü vb buradan yürütülür.</em>",
              paryetellob: "<b>Paryetel Lob</b> <em>Duyuları işleme, şekil ve renkleri algılama, uzaysal algı, görme algısı ve aritmetik yetenekleri kontrol eder.</em>",
              oksipitallob: "<b>Oksipital Lob</b> <em>Görsel bilgiyi işleyen lobdur. Ayrıca okuma da bu lob kontrolü altında gerçekleşir.</em>",

          };

          var defaultDipTooltip = ' ';

          var image = $('#brain');

          image.mapster({
              fillOpacity: 0.4,
              fillColor: "DADDFC",
              stroke: true,
              strokeColor: "EAEAEA",
              strokeOpacity: 0.8,
              strokeWidth: 2,
              singleSelect: true,
              mapKey: 'name',
              listKey: 'name',
              onClick: function(e) {
                  var newToolTip = defaultDipTooltip;

                  // update text depending on area selected
                  $('#selections').html(xref[e.key]);
                  $("#selections").width(getTextWidth(xref[e.key]));
                  $(this).animate({});


                  // if Asparagus selected, change the tooltip
                  if (e.key === 'omurilik') {
                      newToolTip = "OK. I know I have come down on the dip before, but let's be real. " +
                          "Raw asparagus without any of that delicious ranch and onion dressing " +
                          "slathered all over it is not so good.";
                  }
                  image.mapster('set_options', {
                      areas: [{
                          key: "dip",
                          toolTip: newToolTip
                      }]
                  });
              },
              showToolTip: true,
              toolTipClose: ["tooltip-click", "area-click"],
              areas: [{
                  key: "epitalamus",
                  fillColor: "F90716"
              }, {
                  key: "hayatucgeni",
                  fillColor: "FFCA03"
              }, {
                  key: "frontallob",
                  fillColor: "98BAE7"
              }, {
                  key: "hipofiz",
                  fillColor: "98BAE7"
                },{
                  key: "paryetellob",
                  fillColor: "F3950D"
              }, {
                  key: "oksipitallob",
                  fillColor: "F90716"
              }]
          });


        function getTextWidth(text) {
        var e = $('<span></span>'); // dummy element
        e.text(text);
        var width = e.appendTo($('body')).width(); // append and get width
        e.remove(); // remove from DOM
        return width;
      }

