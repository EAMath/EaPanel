      <script src="https://rawgit.com/jamietre/ImageMapster/master/dist/jquery.imagemapster.min.js"></script>
      <script>

          // a cross reference of area names to text to be shown for each area
          var xref = {
              beyincik: "<b>Beyincik</b> <em>Kas hareketlerini düzenler ve vücut dengesini sağlar. çalışmasında iç kulaktan ve gözden gelen uyartılar etkilidir.</em>",
